package modelo;

public class Matematica {

	public static void main(String[] args) {
		int v1 = 13;
		int v2 = 3;
		
		System.out.printf("%d + %d = %d\n",v1,v2,v1+v2);
		System.out.printf("%d - %d = %d\n",v1,v2,v1-v2);
		System.out.printf("%d * %d = %d\n",v1,v2,v1*v2);
		System.out.printf("%d / %d = %d\n",v1,v2,v1/v2);
	}

}
